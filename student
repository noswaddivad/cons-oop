
public class Student {
	
	int StudID, Seatwork, Quizzes, Lab, Project, MajorExam;
	String StudName, Subject;
	
	Student (int studid, String studname, String subject, int seatwork, int quizzes, int lab, int project, int majorexam){
		this.StudID = studid;
		this.StudName = studname;
		this.Subject = subject;
		this.Seatwork = seatwork;
		this.Quizzes = quizzes;
		this.Lab = lab;
		this.Project = project;
		this.MajorExam = majorexam;
	}
	
	public double TermGrade () {
		double Average = (Seatwork *.15 + Quizzes * .15 + Lab * .15 + Project *.15 + MajorExam *.15);
		return Average;
	}
	
}
